<header class="header">
  <div class="container header__container">
    <nav class="nav header-nav">
      <ul class="list header-nav__list">
        <li class="list__item">
          <a
            class="list__link header-nav__link ripple"
            href="#"
            role="button"
            data-ripple-value="true"
            >Главная
            <div class="ripple__container"></div
          ></a>
        </li>
        <li class="list__item">
          <a
            class="list__link header-nav__link ripple"
            href="#"
            role="button"
            data-ripple-value="true"
            >Библиотека
            <div class="ripple__container"></div
          ></a>
        </li>
        <li class="list__item">
          <a
            class="list__link header-nav__link ripple"
            href="#"
            role="button"
            data-ripple-value="true"
            >Ресурсы
            <div class="ripple__container"></div
          ></a>
        </li>
        <li class="list__item">
          <a
            class="list__link header-nav__link header-nav__link--active ripple"
            data-ripple-value="true"
            href="/#"
            role="button"
            >Конференции
            <div class="ripple__container"></div
          ></a>
        </li>
        <li class="list__item">
          <a
            class="list__link header-nav__link ripple"
            href="#"
            role="button"
            data-ripple-value="true"
            >Деканат
            <div class="ripple__container"></div
          ></a>
        </li>
        <li class="list__item">
          <a
            class="list__link header-nav__link ripple"
            href="#"
            role="button"
            data-ripple-value="true"
            >Еще...
            <div class="ripple__container"></div
          ></a>
        </li>
      </ul>
    </nav>
    <div class="user-info header__user-info">
      <a
        class="img-wrapper user-info__img-wrapper"
        href="https://lk.dvgups.ru/profile"
        role="button"
      >
        <img class="img" src="public/img/user.jpg" alt="user" />
      </a>
      <%- include('./theme-switcher') %>
      <a class="user-info__notify ripple" href="#" role="button" data-ripple-value="true">
        <img class="icon" src="img/bell.svg" alt="bell" />
        <div class="ripple__container"></div>
      </a>
      <a class="user-info__exit ripple" href="#" role="button" data-ripple-value="true">
        <img class="icon" src="public/img/log-out.svg" alt="exit" />
        <div class="ripple__container"></div>
      </a>
    </div>

    <%- include('./breadcrumbs') %>
  </div>
</header>
